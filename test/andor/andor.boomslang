
def main()
    var zero = 0
    var one = 1
    
    # Logical Or
    if zero = 0 or zero = 10
        "zero is 0 or 10".output()
    else
        "zero is not 0 or 10".output()
    
    # Logical And
    if zero = 0 and one = 1
        "zero is 0 and one is 1".output()
    else
        "zero is not 0 or one is not 1".output()